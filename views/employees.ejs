                </div>
            </div>
        </div>
    </div>
</div>

<script>
$(document).ready(function () {
          $("#slider").slider(
          {
              min: 18,
              max: 40,
              step: 1,
              change: showValue

          });
          $("#update").click(function () {
              $("#slider").slider("option", "value", $("#seekTo").val());

          });
          function showValue(event, ui) {
              $("#val").html(ui.value);
          }
      });
</script>
