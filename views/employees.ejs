  <div class="loader-div hidden" id="login-loader-div" onclick="hideMyElement('loader-div')">
      <div class="loader">
          <a href="#" class="close-text"><h1 class="text-right close-text" onclick="closeDarkDiv('login-loader-div')">&times;</h1></a>
          <h2 class="text-center sheer-login-text">Login As</h2>
          <br/>
          <br/>
          <a href="/employee_login"><h1 class="text-center sheer-login-link">Maid/Driver/Househelp</h1></a>
          <br>
          <a href="/employer_login"><h1 class="text-center sheer-login-link">Employer</h1></a>
      </div>
  </div>

  <div class="loader-div hidden" id="signup-loader-div" onclick="hideMyElement('loader-div')">
      <div class="loader">
          <a href="#" class="close-text"><h1 class="text-right close-text" onclick="closeDarkDiv('signup-loader-div')">&times;</h1></a>
          <h2 class="text-center sheer-login-text">Signup As</h2>
          <br/>
          <br/>
          <a href="/employee_signup"><h1 class="text-center sheer-login-link">Maid/Driver/Househelp</h1></a>
          <br>
          <a href="/employer_signup"><h1 class="text-center sheer-login-link">Employer</h1></a>
      </div>
  </div>

  <div class="container">
        <div class="row sheerlock-search-div">
            <br/>
            <h3>Select
              <% if (type == 'Auntie') { %>
                   an
              <% } else { %>
                   a
              <% } %>

               <%= type %></h3>
            <br/>
            <li class="dropdown" style="margin-left:auto; margin-right:auto; width: 150px;" id="employees-dropdown">
              <a href="#" class="dropdown-toggle sheerlock-navbar-a" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Employee Type<span class="caret"></span></a>
              <ul class="dropdown-menu" id="employees-dropdown-menu">
                <li><a href="/employees/1/type/Friend" class="yellow_text">Friend</a></li>
                <li><a href="/employees/1/type/Auntie" class="yellow_text">Auntie</a></li>
                <li><a href="/employees/1/type/Granny" class="yellow_text">Granny</a></li>
                <li><a href="/employees/1/type/Maid" class="yellow_text">Maid</a></li>
                <li><a href="/employees/1/type/Driver" class="yellow_text">Driver</a></li>
              </ul>
            </li>
        </div>
        <br/>
        <hr/>
        <br/>
        <div class="row">
          <% for (var i = 0; i < employees.length; i++) { %>
              <div class="col-md-4 col-sm-4">
                  <div class="thumbnail">
                      <img src="<%= employees[i].photo_url %>">
                      <div class="caption">
                          <h3><%= employees[i].firstname %></h3>
                          <p><%= employees[i].type %></p>
                          <% for (var j = 0; j < 5; j++) { %>
                              <% if ((j+1) > employees[i].average_rating) { %>
                                  <span class="fa fa-star"></span>
                              <% } else { %>
                                  <span class="fa fa-star checked"></span>
                              <% } %>
                          <% } %>
                          <br/>
                          <br/>
                          <a class="">Select Me</a>
                      </div>
                  </div>
              </div>
          <% } %>
        </div>
        <% if (pages > 0) { %>
            <ul class="pagination text-center">
                <% if (current == 1) { %>
                    <li class="disabled"><a>First</a></li>
                <% } else { %>
                    <li><a href="/employees/1/type/<%= type %>">First</a></li>
                <% } %>
                <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                <% if (i !== 1) { %>
                    <li class="disabled"><a>...</a></li>
                <% } %>
                <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                    <% if (i == current) { %>
                        <li class="active"><a><%= i %></a></li>
                    <% } else { %>
                        <li><a href="/employees/<%= i %>/type/<%= type %>"><%= i %></a></li>
                    <% } %>
                    <% if (i == Number(current) + 4 && i < pages) { %>
                        <li class="disabled"><a>...</a></li>
                    <% } %>
                <% } %>
                <% if (current == pages) { %>
                    <li class="disabled"><a>Last</a></li>
                <% } else { %>
                    <li><a href="/employees/<%= pages %>/type/<%= type %>">Last</a></li>
                <% } %>
            </ul>
        <% } %>
  </div>
</div>

